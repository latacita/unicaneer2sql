  <audits
      id="Checks"
      name="Feature Model Audit">
    <audits
        name="sizeNameProject"
        description="Check the size's name"
        id="sizeNameProject"
        message="The size's name must be greater than zero">
      <rule
          body="self.name.size() > 0"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Project"/>
      </target>
    </audits>
    <audits
        name="sizeNameNode"
        description="Check the size's name of the node"
        id="sizeNameNode"
        message="The size's name of the node must be greater than zero">
      <rule
          body="self.name.size() > 0"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Node"/>
      </target>
    </audits>
    <audits
        name="NodeName"
        description="Check the Nodes' names"
        id="NodeName"
        message="The Node's name must be unique">
      <rule
          body="Node.allInstances()->forAll(p1, p2 |  p1 &lt;> p2 implies p1.name &lt;> p2.name)"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Node"/>
      </target>
    </audits>
    <audits
        name="sizeNameAttribute"
        description="Check the size's name of the attribute"
        id="sizeNameAttribute"
        message="The size's name of the attribute must be greater than zero">
      <rule
          body="self.name.size() > 0"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Attribute"/>
      </target>
    </audits>
    <audits
        name="AttributeName"
        description="Check the attributes' name "
        id="AttributeName"
        message="The AttributeÂ´s name must be unique">
      <rule
          body="Node.allInstances() -> forAll(n | n.attributes -> forAll(p1, p2 |  p1 &lt;> p2 implies  p1.name &lt;> p2.name))"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Node"/>
      </target>
    </audits>
    <audits
        name="AggregationSource"
        description="Check the source of the Aggregation"
        id="AggregationSource"
        message="The Aggregation's source can not be null">
      <rule
          body="self.source -> notEmpty()"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Participant"/>
      </target>
    </audits>
    <audits
        name="PrimaryKey"
        description="Check the name of the primary key"
        id="PrimaryKey"
        message="The primary key can not be null">
      <rule
          body="self.primaryKey -> notEmpty()"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Entity"/>
      </target>
    </audits>
    <audits
        name="CandidateKey"
        description="Check the attributes of the candidate key"
        id="CandidateKey"
        message="The candidate key can not be null">
      <rule
          body="self.attributes -> notEmpty()"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//CandidateKey"/>
      </target>
    </audits>
    <audits
        name="CompositeAttribute"
        description="Check the attributes of the composite attribute"
        id="CompositeAttribute"
        message="The composite attribute can not be null">
      <rule
          body="self.singlesAttributes -> notEmpty()"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//CompositeAttribute"/>
      </target>
    </audits>
    <audits
        name="DependencyRelationship"
        description="Check the relation between a Dependency Relationship and a Weak Entity"
        id="DependencyRelationship"
        message="A Dependency Relationship must be mached with a Weak Entity">
      <rule
          body="self.participants -> forAll(p1, p2 |  p1.target.oclIsKindOf(WeakEntity) implies p2.target.oclIsKindOf(Entity))"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//DependencyRelationship"/>
      </target>
    </audits>
    <audits
        name="DomainName"
        description="Check the name of the domain"
        id="DomainName"
        message="The name of the domain can not be null">
      <rule
          body="self.name.size() > 0"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//Domain"/>
      </target>
    </audits>
    <audits
        name="DomainLiterals"
        description="Check the literals of the enumerated type"
        id="DomainLiterals"
        message="The literals can not  be null">
      <rule
          body="self.name.size() > 0"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//EnumeratedLiteral"/>
      </target>
    </audits>
    <audits
        name="DomainAttribute"
        description="Check the value of the domain in a Single Attribute"
        id="DomainAttribute"
        message="The domain of  a Single Attribute can not be null.">
      <rule
          body="self.domain &lt;> null"/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//SingleAttribute"/>
      </target>
    </audits>
    <audits
        name="Cardinality"
        description="Check the cardinality of the relationship"
        id="Cardinality"
        message="The upperBound must be > or = than lowerBound.">
      <rule
          body="self.upperBound >= self.lowerBound "/>
      <target
          xsi:type="gmfmap:DomainElementTarget">
        <element
            href="EERmodel.ecore#//AbstractParticipant"/>
      </target>
    </audits>
  </audits>
</gmfmap:Mapping>
