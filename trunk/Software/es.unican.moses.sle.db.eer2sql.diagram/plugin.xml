   <extension point="org.eclipse.emf.validation.constraintProviders">
      <?gmfgen generated="true"?>
            <category id="Checks" mandatory="false" name="Feature Model Audit">
         <![CDATA[]]>
      </category>
      <constraintProvider cache="true">
         <package namespaceUri="http://EER/1.0"/>
               <constraints categories="Checks">
                  <constraint id="sizeNameProject"
            lang="OCL"
            name="sizeNameProject"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.name.size() > 0]]>
            <description><![CDATA[Check the size's name]]></description>
            <message><![CDATA[The size's name must be greater than zero]]></message>
            <target class="EER.Project"/>
         </constraint>
                  <constraint id="sizeNameNode"
            lang="OCL"
            name="sizeNameNode"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.name.size() > 0]]>
            <description><![CDATA[Check the size's name of the node]]></description>
            <message><![CDATA[The size's name of the node must be greater than zero]]></message>
            <target class="EER.Node"/>
         </constraint>
                  <constraint id="NodeName"
            lang="OCL"
            name="NodeName"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[Node.allInstances()->forAll(p1, p2 |  p1 <> p2 implies p1.name <> p2.name)]]>
            <description><![CDATA[Check the Nodes' names]]></description>
            <message><![CDATA[The Node's name must be unique]]></message>
            <target class="EER.Node"/>
         </constraint>
                  <constraint id="sizeNameAttribute"
            lang="OCL"
            name="sizeNameAttribute"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.name.size() > 0]]>
            <description><![CDATA[Check the size's name of the attribute]]></description>
            <message><![CDATA[The size's name of the attribute must be greater than zero]]></message>
            <target class="EER.Attribute"/>
         </constraint>
                  <constraint id="AttributeName"
            lang="OCL"
            name="AttributeName"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[Node.allInstances() -> forAll(n | n.attributes -> forAll(p1, p2 |  p1 <> p2 implies  p1.name <> p2.name))]]>
            <description><![CDATA[Check the attributes' name ]]></description>
            <message><![CDATA[The Attribute´s name must be unique]]></message>
            <target class="EER.Node"/>
         </constraint>
                  <constraint id="AggregationSource"
            lang="OCL"
            name="AggregationSource"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.source -> notEmpty()]]>
            <description><![CDATA[Check the source of the Aggregation]]></description>
            <message><![CDATA[The Aggregation's source can not be null]]></message>
            <target class="EER.Participant"/>
         </constraint>
                  <constraint id="PrimaryKey"
            lang="OCL"
            name="PrimaryKey"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.primaryKey -> notEmpty()]]>
            <description><![CDATA[Check the name of the primary key]]></description>
            <message><![CDATA[The primary key can not be null]]></message>
            <target class="EER.Entity"/>
         </constraint>
                  <constraint id="DependencyRelationship"
            lang="OCL"
            name="DependencyRelationship"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.participants ->exists(e1 | e1.target.oclIsKindOf(WeakEntity))]]>
            <description><![CDATA[Check the relation between a Dependency Relationship and a Weak Entity]]></description>
            <message><![CDATA[A Dependency Relationship must be mached with a Weak Entity]]></message>
            <target class="EER.DependencyRelationship"/>
         </constraint>
                  <constraint id="DomainName"
            lang="OCL"
            name="DomainName"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.name.size() > 0]]>
            <description><![CDATA[Check the name of the domain]]></description>
            <message><![CDATA[The name of the domain can not be null]]></message>
            <target class="EER.Domain"/>
         </constraint>
                  <constraint id="DomainLiterals"
            lang="OCL"
            name="DomainLiterals"
            mode="Batch"
            severity="ERROR" statusCode="200">
            <![CDATA[self.name.size() > 0]]>
            <description><![CDATA[Check the literals of the enumerated type]]></description>
            <message><![CDATA[The literals can not  be null]]></message>
            <target class="EER.EnumeratedLiteral"/>
         </constraint>
                  </constraints>
      </constraintProvider>
   </extension>

